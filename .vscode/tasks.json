{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Tmux",
      "type": "shell",
      "command": "tmuxinator start --suppress-tmux-version-warning=SUPPRESS-TMUX-VERSION-WARNING",
      "isBackground": true,
      "runOptions": {
        "runOn": "folderOpen"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "dedicated",
        "showReuseMessage": false,
        "clear": false
      },
      "problemMatcher": []
    },
  ]
}